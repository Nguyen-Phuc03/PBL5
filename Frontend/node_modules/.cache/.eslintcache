    :   /**\n                                                FOR FULL CLASS\n                                            */\n                                           <table>\n                                                <thead>\n                                                    <tr>\n                                                        <th> Roll No </th>\n                                                        <th> Classes Attended </th>\n                                                        <th> Total Classes </th>\n                                                    </tr>\n                                                </thead>\n                                                <tbody>\n                                                    {\n                                                        Object.keys(attendance).map(roll_no =>{\n                                                            return <tr key={roll_no}>\n                                                                        <td>{roll_no}</td>\n                                                                        <td>{attendance[roll_no]['totalClassesAttended']}</td>\n                                                                        <td>{attendance[roll_no]['totalClasses']}</td>\n                                                                    </tr>\n                                                        })\n                                                    }\n                                                    \n                                                </tbody>\n                                            </table>\n                                    }\n                                    \n                                </div>\n                            </main>\n                        </div>\n                    </div>\n                </div>\n            </React.Fragment>\n        )\n    }\n}\n\n\nconst mapStateToProps = (state)=> {\n    return {\n        stu