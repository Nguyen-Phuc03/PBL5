\n\n            const notAllowed = ['confirmPassword']\n\n            /**\n             * ONLY ALLOW THE DATA WHICH IS NOT IN \n             * NOT ALLOWED ARRAY\n             */\n            const filterData = Object.keys(data)\n                               .filter(key => notAllowed.includes(key) ? false : true)\n                               .reduce((obj, key)=>{\n                                    obj[key] = data[key]\n                                    return obj;\n                               },{});\n\n            makeRequest(requestUrl,filterData)\n            .then(response => response.json())\n            .then(response => this.handleResponse(response))\n        }else{\n            /**\n             * Errors should not stay on page forever,\n             * So after 3sec of errors are showen on The Page,\n             * I should call clearAllErrors() to clear them \n             */\n            this.errorTimeOut = setTimeout(()=>{\n                this.clearAllErrors();\n            },3000)\n        }\n    })\n\n}","C:\\Users\\USER\\Downloads\\PBL5_Demo\\PBL5_Demo\\Frontend\\src\\components\\BeforeSigninLinks\\BeforeSigninLinks.js",[],"C:\\Users\\USER\\Downloads\\PBL5_Demo\\PBL5_Demo\\Frontend\\src\\components\\AfterSigninLinks\\AfterSigninLinks.js",[],"C:\\Users\\USER\\Downloads\\PBL5_Demo\\PBL5_Demo\\Frontend\\src\\genericFunctions\\makeRequest.js",["206"],"export default function(reqeustUrl, dataToSend={}){\n    const options = {\n        method: 'POST',\n        headers: {\n            'Content-Type': 'application/json'\n        },\n        /**\n         * EVEN IF dataToSend IS EMPTY IT WILL \n         * SIMPLY SEND AND EMPTY OBJECT & IT WILL WORK\n         */\n        body: JSON.stringify(dataToSend)\n\n    }\n    return fetch(reqeustUrl,options)\n}","C:\\Users\\USER\\Downloads\\PBL5_Demo\\PBL5_Demo\\Frontend\\src\\helperFunction\\logout.js",["207"],"import Cookies from 'universal-cookie';\n\n\nexport default  ()=>{\n    const cookies = new Cookies